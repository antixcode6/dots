# Set personal aliases, overriding those provided by oh-my-zsh libs,
# plugins, and themes. Aliases can be placed here, though oh-my-zsh
# users are encouraged to define aliases within the ZSH_CUSTOM folder.
# For a full list of active aliases, run `alias`.
#
# Example aliases
# alias zshconfig="mate ~/.zshrc"
# alias ohmyzsh="mate ~/.oh-my-zsh"
plugins=(golang git brew)

# GKE Config
alias gke-test='gcloud config configurations activate test && kubectx gke-test'
alias gke-dev='gcloud config configurations activate dev && kubectx gke-dev'
alias gke-prod='gcloud config configurations activate prod && kubectx gke-prod'

# AWS K8S
alias aws-test='okta-awscli --profile test --force --okta-profile test && export AWS_DEFAULT_PROFILE=test && kubectx eks-test'
alias eks-dev='okta-awscli --profile dev --force --okta-profile dev && export AWS_DEFAULT_PROFILE=dev && kubectx eks-dev'
alias aws-prod='okta-awscli --profile prod --force --okta-profile prod && export AWS_DEFAULT_PROFILE=prod && kubectx eks'

#Launch lvim with nvim
alias nvim=lvim

#Git Aliases
alias lgl="git log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all"
#alias branch-blame="git for-each-ref --format='%(committerdate)%09%(authorname)%09%(refname)' | sort -k5n -k2M -k3n -k4n | grep remotes | awk -F "\"\t"\" '{ printf "\"%-32s %-27s %s\n"\", $1, $2, $3 }'"

#Kubectl Aliases
alias kdp="kubectl describe pod"
alias kgp="kubectl get pods"
alias kgd="kubectl get deployments"
alias kl="kubectl logs"
alias kdd="kubectl describe deployment"
alias kms="kubectl modify-secret"
alias kmd="kubectl edit deployment"
alias kmp="kubectl edit pod"
alias kprm="kubectl delete pod"

# To customize prompt, run `p10k configure` or edit ~/.p10k.zsh.
[[ ! -f ~/.p10k.zsh ]] || source ~/.p10k.zsh

export GOOGLE_APPLICATION_CREDENTIALS="/Users/eddie.aponte/dev.json"
export USE_GKE_GCLOUD_AUTH_PLUGIN=True
export PATH="${KREW_ROOT:-$HOME/.krew}/bin:$PATH"
[[ $commands[kubectl] ]] && source <(kubectl completion zsh)
